{
  "name": "vite-plugin-paraglide-editor",
  "version": "0.1.0",
  "description": "Vite plugin that provides an in-browser translation editor for ParaglideJS",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./sveltekit": {
      "types": "./src/sveltekit.d.ts",
      "default": "./src/sveltekit.js"
    }
  },
  "files": [
    "src",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "vite",
    "plugin",
    "paraglide",
    "i18n",
    "internationalization",
    "editor",
    "metadata",
    "translation",
    "localization"
  ],
  "author": "Paraglide Editor Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/francoislg/paraglide-browser-addon.git",
    "directory": "packages/vite-plugin-paraglide-editor"
  },
  "homepage": "https://github.com/francoislg/paraglide-browser-addon#readme",
  "bugs": {
    "url": "https://github.com/francoislg/paraglide-browser-addon/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "echo \"No tests yet. Run examples to verify.\" && exit 0",
    "prepublishOnly": "echo \"⚠️  Publishing vite-plugin-paraglide-editor@$npm_package_version\" && sleep 2"
  },
  "peerDependencies": {
    "@inlang/paraglide-js": "^2.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  }
}
