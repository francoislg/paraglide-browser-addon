{
  "name": "vite-plugin-paraglide-debug",
  "version": "0.1.0",
  "description": "Vite plugin that injects debug metadata into ParaglideJS translation strings",
  "type": "module",
  "main": "./src/index.js",
  "types": "./client.d.ts",
  "exports": {
    ".": {
      "types": "./client.d.ts",
      "default": "./src/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "files": [
    "src",
    "runtime.js",
    "client.d.ts"
  ],
  "keywords": [
    "vite",
    "plugin",
    "paraglide",
    "i18n",
    "debug",
    "metadata"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@inlang/paraglide-js": "^2.0.0",
    "vite": "^5.0.0 || ^6.0.0"
  }
}
