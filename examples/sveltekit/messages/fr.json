      "platform=*": "Plateforme inconnue"
    }
  }],
  "user_activity": [{
    "declarations": [
      "input count",
      "input gender",
      "local countPlural = count: plural"
    ],
    "selectors": ["countPlural", "gender"],
    "match": {
      "countPlural=one, gender=male": "Il a publié {count} photo",
      "countPlural=one, gender=female": "Elle a publié {count} photo",
      "countPlural=one, gender=*": "Ils ont publié {count} photo",
